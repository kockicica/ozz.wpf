        <SolidColorBrush x:Key="ComboBoxBackgroundPointerOver" Color="{DynamicResource ThemeLow}"/>
        <SolidColorBrush x:Key="ComboBoxBackgroundFocused" Color="{DynamicResource ThemeLow}"/>
        

